<?xml version='1.0' encoding='utf-8'?>
<!--
  Modified network_security_config.xml
  - Adds <debug-overrides> to allow user-installed CAs for debugging/interception.
  - USE ONLY IN A TESTING ENVIRONMENT. Do NOT ship to production.
-->
<network-security-config>
  <!-- default / base behaviour -->
  <base-config cleartextTrafficPermitted="true">
    <trust-anchors>
      <certificates src="system" />
    </trust-anchors>
  </base-config>

  <!-- debug-overrides: allows user CAs (useful for Burp/mitmproxy interception on debug builds).
       This block is only used when the app is debuggable or when device config allows it.
       Keep this only in test builds. -->
  <debug-overrides>
    <trust-anchors>
      <!-- allow user-installed CA certs + system certs during debugging -->
      <certificates src="user" />
      <certificates src="system" />
    </trust-anchors>
  </debug-overrides>

